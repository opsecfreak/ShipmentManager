{
  "name": "shipmentmanager",
  "version": "1.0.0",
  "type": "module",
  "description": "A Local bot that uses prisma, zod, and openai agents to help keep track of day to day business activities with a focus on technician duties and shipments/tasks.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "tsx src/test.ts",
    "demo": "tsx src/demo.ts",
    "comprehensive-demo": "tsx src/comprehensive-demo.ts",
    "quickstart": "tsx src/quickstart.ts",
    "check": "tsx src/runtime-check.ts",
    "lint": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "db:seed": "tsx prisma/seed.ts",
    "postinstall": "prisma generate"
  },
  "keywords": [
    "prisma",
    "shipment",
    "management",
    "business",
    "tasks"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "@prisma/extension-accelerate": "^2.0.2",
    "dotenv": "^16.3.1",
    "uuid": "^10.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@types/uuid": "^10.0.0",
    "prisma": "^5.22.0",
    "tsx": "^4.6.2",
    "typescript": "^5.9.2"
  }
}
